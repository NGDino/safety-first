<button type="button" id="add-business">Add A business</button>
<div>
  <form class="new-business-form hide search-form">
    <div>
      <label for="business-name">Business Name</label>
      <input type="text" id="business-name" class="search" placeholder="Business Name">
      <label for="business-url">Business Website</label>
      <input type="text" id="business-url" name="business-url" placeholder="www.example.com" />
        <div class="input-field col s12">
          <select id="category">
            <option value="" disabled selected>Choose your option</option>
              <option value="1">Food</option>
              <option value="2">Coffee</option>
              <option value="3">Bar/Brewery/Winery</option>
              <option value="4">Retail</option>
              <option value="5">Entertainment</option>
              <option value="6">Other</option>
          </select>
          <label>Category</label>
    </div>
    <button type="submit" class="btn">Create</button>
  </form>
</div>



<ul class="business-list">
  <div class="row">
    <div class="col s12 m12">
      <div class="card-panel black-text yellow lighten-5 center-align ">
        {{!-- <div class="card-panel"> --}}
         {{!-- <div class="title black-text z-depth-2 text-darken-2 "> --}}
        {{#each businesses as |business| }}
        <li>
          {{> business-info business }}
        </li>
        {{/each}}
      </div>
    </div>
  </div>
</ul>


<script src="/javascript/biz-form.js"></script>


